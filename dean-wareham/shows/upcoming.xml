---
layout: null
title: Upcoming Dean Wareham shows
description: Upcoming shows for Dean Wareham.
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ page.title| xml_escape }}</title>
		<description>{{ site.description | append: page.description| xml_escape }} </description>
		<link>{{ site.url }}{{ site.baseurl }}/</link>
		<atom:link href="{{ "/dean-wareham/shows/upcoming.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
		<pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		<generator>Jekyll v{{ jekyll.version }}</generator>
{% assign curdate = site.time | date: '%s' %}{% assign curdate = curdate | plus: 0 %}{% for show in site.dean-wareham-shows %}{% assign showdate = show.show-date | date: '%s' %}{% assign showdate = showdate | plus: 0 %}{% if showdate >= curdate %}{% capture newtitle %}{{show.show-date|date: "%-d %B %Y"}} - Dean Wareham at {{ show.show-venue }}{% endcapture %}
		<item>
			<title>{{ newtitle | xml_escape }}</title>
			<description>{{ show.notes | xml_escape }}</description>
			<pubDate>{{ show.last_modified_at | date_to_rfc822 }}</pubDate>
			<link>{{ show.url | prepend: site.baseurl | prepend: site.url }}</link>
			<guid isPermaLink="true">{{ show.url | prepend: site.baseurl | prepend: site.url }}</guid>
		</item>{% endif %}{% endfor %}
	</channel>
</rss>
